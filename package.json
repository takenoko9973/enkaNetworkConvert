{
    "name": "Enka.Network_lang-jp_mod_by_takenoko",
    "version": "1.1.0",
    "description": "Enka.Network 日本語化スクリプト",
    "author": "Takenoko-ya",
    "typeRoots": [
        "types",
        "node_modules/@types"
    ],
    "scripts": {
        "prebuild": "yarn run clean",
        "build": "rollup --config rollup.config.ts --configPlugin typescript",
        "postbuild": "yarn run lint:userscripts",
        "clean": "rimraf dist",
        "predev": "yarn run clean",
        "dev": "rollup --config rollup.config.ts --configPlugin typescript --watch",
        "lint": "eslint src/**/*.{js,ts} --fix",
        "lint:userscripts": "eslint --no-eslintrc --config .eslintrc.userscripts.json dist/**/*.js"
    },
    "repository": "git+https://github.com/takenoko9973/enkaNetworkConvert.git",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/takenoko9973/enkaNetworkConvert/issues"
    },
    "homepage": "https://github.com/takenoko9973/enkaNetworkConvert#readme",
    "devDependencies": {
        "@rollup/plugin-typescript": "^11.0.0",
        "@tsconfig/node18": "^1.0.1",
        "@types/glob": "^8.0.0",
        "@types/node": "^18.11.18",
        "@types/tampermonkey": "^4.0.7",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@typescript-eslint/parser": "^5.48.1",
        "eslint": "^8.31.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-userscripts": "^0.2.10",
        "glob": "^8.0.3",
        "prettier": "^2.8.2",
        "rimraf": "^4.0.4",
        "rollup": "^3.10.0",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-watch": "^1.0.2",
        "typescript": "^5.1.6",
        "userscript-metadata": "^1.1.0"
    }
}